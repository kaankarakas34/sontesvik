{
  "version": 2,
  "buildCommand": "npm run build --workspace=web",
  "outputDirectory": "web/dist",
  "installCommand": "npm install",
  "framework": null,
  "functions": {
    "web/src/**": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}